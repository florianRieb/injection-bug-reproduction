package de.netzebw.util.to.enums;

import lombok.Getter;
import lombok.RequiredArgsConstructor;

/**
 * The Enum ReporterRole.
 *
 * @author Florian, Riebandt, CGI
 * @version 0.0.1
 * @since
 */
@RequiredArgsConstructor
public enum ReporterRole {

  MAJOR("Bürgermeister"),
  CHAMBERLAIN("Kämmerer"),
  BUILDINGDEPTLEAD("Bauamtsleiter"),
  TIMBERYARDLEAD("Bauhofsleiter"),
  TIMBERYARDEMPLOYEE("Bauhofsmitarbeiter"),
  
  MECHANIC("Monteur"),
  BUSINESSSALES("Fachvertrieb"),
  MUNICIPCONSULTANT("Kommunalberater"),
  CONSTRUCTIONCOORDINATOR("Baukoordinator"),
  CONTROLCENTER("Leitstelle"),
  COORDINATORSPECIALTASK("Koordinator Sonderaufgaben"),
  
  OTHER("Sonstige");

  @Getter
  private final String germanName;

}
